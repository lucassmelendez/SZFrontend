{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "regions": ["iad1"],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://spinzone-backend.vercel.app/api"
  }
} 